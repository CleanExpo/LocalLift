{
  "version": 2,
  "public": true,
  "outputDirectory": "public",
  "github": {
    "enabled": false
  },
  "cleanUrls": true,
  "trailingSlash": true,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "API_URL": "https://humorous-serenity-locallift.up.railway.app",
    "NODE_ENV": "production"
  }
}
